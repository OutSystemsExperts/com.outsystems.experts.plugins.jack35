<?xml version="1.0" encoding="utf-8"?>
<plugin id="com.outsystems.experts.plugins.jack35" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>Jack35Plugin</name>
    <js-module name="Jack35Plugin" src="www/Jack35Plugin.js">
        <clobbers target="cordova.plugins.Jack35Plugin" />
    </js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="Jack35Plugin">
                <param name="android-package" value="com.outsystems.experts.plugins.jack35.Jack35Plugin" />
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml"></config-file>
        <source-file src="src/android/Jack35Plugin.java" target-dir="src/com/outsystems/experts/plugins/jack35/Jack35Plugin" />
    </platform>
</plugin>